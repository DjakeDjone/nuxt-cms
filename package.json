{
  "name": "nuxt-data-mngr",
  "version": "0.1.10",
  "description": "A Nuxt module for creating editable content",
  "repository": "https://github.com/DjakeDjone/nuxt-cms",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "npx nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint -- --fix && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "npm run lint:fix",
    "setup:hooks": "node -e \"const fs=require('fs');const path='.git/hooks/pre-push';const content=fs.readFileSync('scripts/pre-push-hook.sh','utf8');fs.writeFileSync(path,content);fs.chmodSync(path,0o755);console.log('âœ… Pre-push hook installed!');\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/icon": "^1.15.0",
    "@nuxt/kit": "^4.0.1",
    "@tailwindcss/vite": "^4.1.11",
    "@tiptap/extension-bubble-menu": "^3.0.7",
    "@tiptap/extension-text-align": "^3.0.7",
    "@tiptap/extension-underline": "^3.0.7",
    "menus": "link:@tiptap/vue-3/menus",
    "nuxt-tiptap-editor": "^2.3.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.2",
    "@nuxt/eslint-config": "^1.7.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.1",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.32.0",
    "nuxt": "^4.0.1",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4",
    "prettier": "^2.8.8"
  },
  "packageManager": "pnpm@10.14.0"
}